# ========================================
# PRODUCTION ENVIRONMENT VARIABLES
# ========================================

# Database Configuration
POSTGRES_DB=vacation_db
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_secure_db_password_here

# Django Configuration
DJANGO_SECRET_KEY=your_django_secret_key_here_change_this_in_production
DJANGO_DEBUG=False
DJANGO_ALLOWED_HOSTS=your-ec2-public-ip,your-domain.com,localhost,127.0.0.1
STATIC_URL=/static/
MEDIA_URL=/media/

# Flask Configuration  
JWT_SECRET_KEY=your_jwt_secret_key_here_change_this_in_production
FLASK_ENV=production
CORS_ORIGINS=http://your-ec2-public-ip,https://your-domain.com

# React Configuration
REACT_APP_API_URL=http://your-ec2-public-ip/api
REACT_APP_DJANGO_URL=http://your-ec2-public-ip/django

# Logging
LOG_LEVEL=WARNING

# Security
SECURE_SSL_REDIRECT=False
SESSION_COOKIE_SECURE=False
CSRF_COOKIE_SECURE=False

